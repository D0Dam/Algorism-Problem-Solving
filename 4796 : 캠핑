#include<iostream>
#include<vector>
using namespace std;

int main()
{
    int a = 1;
    vector<int> v;
    while(a != -1)
    {
        int arr[3];
        for(int i = 0; i < 3; i++)
        {
            cin >> arr[i];
            v.push_back(arr[i]);
        }
        if(arr[0] == 0 && arr[1] == 0 && arr[2] == 0)
            a=-1;
    }

    return 0;
}

void res(int L, int P, int V)
{
    int result = 0;
    result = (V/P)*L;
    if(V-result > L)
    {
        result = V-result
    }
}
