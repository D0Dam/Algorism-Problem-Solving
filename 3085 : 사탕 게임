#include<iostream>
using namespace std;
void swap_down();
void swap_rigt();

int main()
{
    int N;
    cin >> N;
    int **arr = new int*[N];
    for(int i = 0; i < N; i++)
        arr[i] = new int[N];
    for(int i = 0; i < N; i++)
    {
        for(int j = 0; j < N; j++)
        {
            cin >> arr[i][j];
        }
    }
    for(int i = 0; i < N; i++)
    {
        for(int j = 0; j < N; j++)
        {  
            if(i < N - 1)
            {
                swap_down();
                if(j < N -1)
                {
                    swap_right();
                }
            }
        }
    }
    for(int i = 0; i < N; ++i)
    { 
        delete [] arr[i];
    }
     delete [] arr;

    return 0;
}

